#!/bin/bash

scriptDir="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )"

if [ -z "$scriptDir" ]; then
  printf \
'Error, script unable to locate its directory. Exiting...\n'
fi

for jar in $(ls $scriptDir/../lib); do
  CLASSPATH=$CLASSPATH:$scriptDir/../lib/$jar
done

java -jar $scriptDir/../lib/aldb.jar $@
